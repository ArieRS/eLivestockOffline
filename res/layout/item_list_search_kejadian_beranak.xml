<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/tvNITBeranak"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:text="NIT Beranak"
        android:textColor="@color/black"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/tvTanggalBeranak"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Tanggal Beranak"
        android:textColor="@color/black"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/tvJumlahAnakBetina"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Anak Betina Beranak"
        android:textColor="@color/black"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/tvJumlahAnakJantan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:text="Anak Jantan Beranak"
        android:textColor="@color/black"
        android:textSize="16sp" />
</LinearLayout>